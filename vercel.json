{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*\\.(?:html|css|js|gif|jpg|png|svg))$",
      "dest": "/dist/pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
